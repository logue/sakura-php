{
    "name": "logue/sakura-php",
    "description": "PHP keyword file generator for Sakura Editor.",
    "version": "0.0.0",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/logue/sakura-php"
        }
    ],
    "require": {
        "ext-zlib": "*",
        "php": "^7.0",
        "symfony/console": "^4.3",
        "zendframework/zend-dom": "^2.7",
        "zendframework/zend-http": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/var-dumper": "^4.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Logue",
            "email": "logue@hotmail.co.jp"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Logue\\SakuraPhp\\" : "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check": "vendor/bin/phpcs",
        "fix": "vendor/bin/phpcbf"
    }
}
